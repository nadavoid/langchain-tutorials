{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch rag-conversatonal via Yarn",
      "request": "launch",
      "runtimeArgs": [
        "tsx"
        // "run-script",
        // "debug"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],

      "program": "${workspaceFolder}/rag-conversational/index.ts",
      "type": "node",
      "cwd": "${workspaceFolder}/rag-conversational"
    },

    {
      "name": "Launch rag-agent via Yarn",
      "request": "launch",
      "runtimeArgs": [
        "tsx"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],

      "program": "${workspaceFolder}/rag-conversational/agent.ts",
      "type": "node",
      "cwd": "${workspaceFolder}/rag-conversational"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Launch RAG",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/rag/rag.js",
      "cwd": "${workspaceFolder}/rag"
    }
  ]
}
